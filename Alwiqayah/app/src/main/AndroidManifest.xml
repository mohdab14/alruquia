<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mj7addadcoder.alwiqayah">


    <uses-sdk tools:overrideLibrary="com.daimajia.slider.library,com.google.android.gms.appindexing,com.google.android.gms"
        />



    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_app"
        android:label="حزب الوقاية والحفظ"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name=".Splash"
            android:screenOrientation="portrait"
            android:label="حزب الوقاية والحفظ"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="حزب الوقاية والحفظ"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name=".OneActivity"
            android:label="حزب الوقاية والحفظ"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.mj7addadcoder.alwiqayah.OneActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>